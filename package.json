{
  "name": "trip-yoetz",
  "description": "A guidance platform for planning a trip based on guidance of other users",
  "version": "1.0.0",
  "main": "server/server.js",
  "author": "marcos bazbih",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "init": "^0.1.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.4",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "npm install && cd client && npm install && npm run build",
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "cd client && npm run start",
    "clientinstall": "npm install --prefix client",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "render-postbuild": "NPM_CONFIG_PRODUCTION=false cd client && npm install && npm run build"
  }
